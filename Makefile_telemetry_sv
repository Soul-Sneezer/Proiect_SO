# Compiler and flags
CC = gcc
CFLAGS = -Wall -Wextra -g
LDFLAGS =

# Source files
SRCS = \
    ./rw_func.c \
	./telemetry_sv.c \
	./error_functions.c \
	./dynamic_list.c 

# Header files
HDRS = \
    ./rw_func.h \
	./error_functions.h \
    ./tlpi_hdr.h \
	./dynamic_list.h \
    ./telemetry_sv.h

# Output executable
TARGET = telemetry_sv

# Default target
all: $(TARGET)

# Build target
$(TARGET): $(SRCS) $(HDRS)
	$(CC) $(CFLAGS) $(LDFLAGS) $(SRCS) -o $(TARGET)

# Clean up
clean:
	rm -f $(TARGET)
